package com.bluemetrix.storeapp.avatar;

public class CheckoutItem 
{	
	private int garmentImage;
	private String title;
	private String points;
	
	public CheckoutItem()
	{
		
	}
	
	public CheckoutItem(int garmentImage, String title, String points)
	{
		this.garmentImage = garmentImage;
		this.title = title;
		this.points = points;
	}
	
	public int getGarmentImage() 
	{
		return garmentImage;
	}

	public void setGarmentImage(int garmentImage)
	{
		this.garmentImage = garmentImage;
	}

	public String getTitle()
	{
		return title;
	}

	public void setTitle(String title) 
	{
		this.title = title;
	}

	public String getPoints() 
	{
		return points;
	}

	public void setPoints(String points)
	{
		this.points = points;
	}
}
